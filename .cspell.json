{
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/coverage/**",
    ".vscode",
    ".gitignore",
    "package-lock.json",
    "report",
    "dist/**"
  ],
  "language": "en",
  "noConfigSearch": true,
  "allowCompoundWords": true,
  "words": ["Typeorm", "codeql", "isready", "nestjs", "typeorm", "venv", "typescript", "Dockerised", "Gabriel", "Ignat"],
  "dictionaries": ["en-gb", "companies", "softwareTerms", "misc", "lorem-ipsum", "typescript", "node", "bash", "npm"],
  "languageSettings": [
    {
      "languageId": "commit-msg",
      "ignoreRegExpList": ["/^#.*/gm"]
    }
  ],
  "version": "0.2"
}
