    "lint:fix": "eslint ./src --ext .ts --fix",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --tsc --watch",
    "start:prod": "node dist/src/main",
    "test": "jest"
  },
  "dependencies": {
