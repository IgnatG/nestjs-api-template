  "author": "Gabriel Ignat",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copyfiles",
    "build:prod": "rimraf dist/ && npm run build",
    "clean": "rimraf dist/",
    "copyfiles": "copyfiles -u 1 public/* dist/public",
